#include <vcl.h>
#pragma hdrstop

#include "Unit2.h"
#include "Unit3.h" // Ajouter Parking form
#include "Unit4.h" // Ajouter Etage form
#include "Unit5.h" // Ajouter Place form
#include "Unit6.h" // Réserver Place form
#include "Unit7.h" // Afficher Places Disponibles form
#include "Unit8.h" // Afficher Places Non Disponibles form

#pragma package(smart_init)
#pragma resource "*.dfm"
TForm2 *Form2;

// Constructor
__fastcall TForm2::TForm2(TComponent* Owner)
    : TForm(Owner)
{
}

// "Ajouter Parking" button click event
void __fastcall TForm2::Button1Click(TObject *Sender)
{
    Form3->Show(); // Show the "Ajouter Parking" form
}

// "Ajouter Etage" button click event
void __fastcall TForm2::Button2Click(TObject *Sender)
{
    Form4->Show(); // Show the "Ajouter Etage" form
}

// "Ajouter Place" button click event
void __fastcall TForm2::Button3Click(TObject *Sender)
{
    Form5->Show(); // Show the "Ajouter Place" form
}

// "Réserver Place" button click event
void __fastcall TForm2::Button4Click(TObject *Sender)
{
    Form6->Show(); // Show the "Réserver Place" form
}

// "Afficher Places Disponibles" button click event
void __fastcall TForm2::Button5Click(TObject *Sender)
{
    Form7->Show(); // Show the "Afficher Places Disponibles" form
}

// "Afficher Places Non Disponibles" button click event
void __fastcall TForm2::Button6Click(TObject *Sender)
{
    Form8->Show(); // Show the "Afficher Places Non Disponibles" form
}
